@using Objects.Sudoku
<td class="tile">
    @if (Tile.CurrentValue > 0)
    {
        <table>

            @for (int i = 1; i <= 3; i++)
            {
                <tr class>
                    @for (int j = 1; j <= 3; j++)
                    {
                        if (Tile.PossibleValues.Contains((i - 1) * 3 + j))
                        {
                            <td class="tile-suggestion">@((i-1)*3+j)</td>
                        }
                    }
                </tr>
            }

        </table>
    }
    else
    {
        <input type="text" maxlength="1" oninput="this.value=this.value.replace(/[^0-9]/g,'');" />
    }


</td>

@code {
    [Parameter]
    public SudokuTile Tile { get; set; }

    public void dosomething()
    {
        Console.Write("asdasd");
    }
}
